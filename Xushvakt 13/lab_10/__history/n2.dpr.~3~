program n2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,System.Math;
var
massiv:array[1..10] of integer;
i,j,k:integer;
s,ss:string;
begin
Randomize;
k:=1;
for i := 1 to 10 do begin
  massiv[i]:=RandomRange(7,33);
  k:= massiv[i]*k;
  s:= s + IntToStr(massiv[i]) + ' ';
end;
writeln('Массив состоит из: ',s);
writeln(k);




readln;
end.
