program n1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
var
chislo,a,b,c:integer;
begin
  write('Введите двузначное число: ');
  Readln(chislo);
  b:= chislo mod 10;
  a:= chislo div 10;
  c:=a+b;
  if c>=10 then begin
    writeln('Сумма цифр числа ',chislo,' является двузначным');
  end else writeln('Сумма цифр числа ',chislo,' является однозначным');

  readln;
end.
