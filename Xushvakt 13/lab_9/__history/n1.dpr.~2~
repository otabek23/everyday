program n1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;
var
i,j,k:integer;
a:Boolean;
s:string;
begin
a:= True;


while a=True do begin
  write('Введите число: ');
  readln(j);
  if j>0 then begin
    for i := 1 to j do begin
      s:=s+IntToStr(i)+' ';
    end;
  end else a=False;writeln('Вы ввели некорректное число!');
  Writeln(s);
end;





readln;
end.
